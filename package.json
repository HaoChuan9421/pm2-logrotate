{
  "name": "@haochuan9421/pm2-logrotate",
  "version": "2.7.0",
  "description": "Module to rotate logs of every pm2 application",
  "main": "app.js",
  "dependencies": {
    "graceful-fs": "^4.2.8",
    "moment-timezone": "^0.5.33",
    "node-schedule": "^2.0.0",
    "pm2": "^5.1.1",
    "pmx": "^1.6.7"
  },
  "author": "郑昊川",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/haochuan9421/pm2-logrotate.git"
  },
  "bugs": {
    "url": "https://github.com/haochuan9421/pm2-logrotate/issues"
  },
  "homepage": "https://github.com/haochuan9421/pm2-logrotate",
  "license": "MIT",
  "apps": [
    {
      "name": "pm2-logrotate",
      "script": "app.js",
      "max_memory_restart": "500M"
    }
  ],
  "config": {
    "max_size": "256M",
    "retain": "7",
    "compress": false,
    "dateFormat": "YYYY-MM-DD-HH-mm-ss",
    "workerInterval": "30",
    "rotateInterval": "0 0 0 * * *",
    "rotateModule": false
  }
}
